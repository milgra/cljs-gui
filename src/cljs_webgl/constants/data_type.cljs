(ns cljs-webgl.constants.data-type
  (:refer-clojure :exclude [byte
                            short
                            int
                            float]))

(def byte           0x1400)
(def unsigned-byte  0x1401)
(def short          0x1402)
(def unsigned-short 0x1403)
(def int            0x1404)
(def unsigned-int   0x1405)
(def float          0x1406)
